<?xml version='1.0' encoding='utf-8'?>
<ns0:style xmlns:ns0="http://purl.org/net/xbiblio/csl" class="in-text" version="1.0.2" default-locale="uk-UA" citation-format="numeric">
  
  <ns0:info>
    <ns0:title>DSTU 8302-2015</ns0:title>
    <ns0:id>http://www.zotero.org/styles/dstu-8302-2015</ns0:id>
    <ns0:link href="http://www.zotero.org/styles/dstu-8302-2015-drradger" rel="self" />
    <ns0:author>
      <ns0:name>Mykhailo Shevchuk</ns0:name>
      <ns0:email>mail@mshevchuk.com</ns0:email>
      <ns0:uri>https://github.com/myshevchuk/dstu-csl</ns0:uri>
    </ns0:author>
